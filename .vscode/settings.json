{
    "configurations": [
        {
            "name": "C build",
            "type": "cppbuild",
            "request": "build",
            "buildRoot": "${workspaceRoot}/build",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-g",
                "-O0"
            ],
            "includePath": [
                "${workspaceFolder}"
            ],
            "defines": [],
            "browse": {
                "path": [
                    "${workspaceRoot}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
	"version": 4,
	"files.associations": {
		"philo.h": "c",
		"stdio.h": "c"
	}
}
